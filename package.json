{
    "name": "jabref-lsp-extension",
    "publisher": "JabRef e.V.",
    "version": "0.0.1",
    "engines": { "vscode": "^1.80.0" },
    "activationEvents": ["onLanguage:bibtex"],
    "main": "./src/extension.js",
    "contributes": {
      "languages": [{
        "id": "bibtex",
        "extensions": [".bib", ".bibtex"]
      }],
      "configuration": {
      "title": "JabRef 4 VSCode",
      "properties": {
        "jabref.consistencyCheck": {
          "type": "boolean",
          "default": true,
          "description": "Enable consistency check"
        },
        "jabref.integrityCheck": {
          "type": "boolean",
          "default": true,
          "description": "Enable integrity check"
        }
      }
    }
    },
    "dependencies": {
      "vscode-languageclient": "^9.0.1"
    }
  }
  